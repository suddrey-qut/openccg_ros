      <complexcat>
        <atomcat type="s">
          <fs id="1">
            <feat attr="index">
              <lf>
                <nomvar name="E"/>
              </lf>
            </feat>
          </fs>
        </atomcat>
        <slash dir="\" mode="&lt;"/>
        <atomcat type="np">
