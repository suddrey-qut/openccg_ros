<family name="state-verbs" pos="VS">
  <entry name="primary">
    <complexcat>
      <atomcat type="s">
        <fs inheritsFrom="2">
          <feat attr="index">
            <lf>
              <nomvar name="E" />
            </lf>
          </feat>
        </fs>
      </atomcat>
      <slash dir="/" mode="&gt;" />
      <atomcat type="adj">
        <fs id="3">
          <feat attr="index">
            <lf>
              <nomvar name="M" />
            </lf>
          </feat>
        </fs>
      </atomcat>
      <slash dir="\" mode="&lt;" />
      <atomcat type="np">
        <fs id="2">
          <feat attr="case" val="nom"/>
          <feat attr="num">
            <featvar name="NUM" />
          </feat>
          <feat attr="index">
            <lf>
              <nomvar name="X" />
            </lf>
          </feat>
        </fs>
      </atomcat>
      <lf>
        <satop nomvar="E:assert">
          <prop name="[*DEFAULT*]"/>
          <diamond mode="arg0">
            <nomvar name="X:sem-obj" />
            <diamond mode="mod">
              <nomvar name="M:state" />
            </diamond>
          </diamond>
        </satop>
      </lf>
    </complexcat>
  </entry>
</family>

